{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      formData: {\n        account: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ account Â≠óÊÆµ\n        name: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ name Â≠óÊÆµ\n        phone: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ phone Â≠óÊÆµ\n        email: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ email Â≠óÊÆµ\n        description: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ description Â≠óÊÆµ\n        password: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ password Â≠óÊÆµ\n        confirmPassword: '' // Áî®‰∫éÁ°ÆËÆ§ÂØÜÁ†ÅÂåπÈÖç\n      },\n      rules: {\n        account: [{\n          required: true,\n          message: 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          message: 'Áî®Êà∑ÂêçËá≥Â∞ëÈúÄË¶Å6‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^[A-Za-z0-9_]{6,}$/,\n          message: 'Áî®Êà∑ÂêçÂè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠óÂíå‰∏ãÂàíÁ∫ø',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞Ëá≥Â∞ëÈúÄË¶Å3‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^[A-Za-z0-9_\\u4e00-\\u9fa5]{3,}$/,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞Âè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠ó„ÄÅ‰∏ãÂàíÁ∫øÂíå‰∏≠ÊñáÂ≠óÁ¨¶',\n          trigger: 'blur'\n        }],\n        phone: [\n        // { required: true, message: 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n        {\n          pattern: /^1[3-9]\\d{9}$/,\n          message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑',\n          trigger: 'blur'\n        }],\n        email: [\n        // { required: true, message: 'ÈÇÆÁÆ±‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n        {\n          type: 'email',\n          message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÈÇÆÁÆ±Âú∞ÂùÄ',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 8,\n          message: 'ÂØÜÁ†ÅËá≥Â∞ëÈúÄË¶Å8‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/,\n          message: 'ÂØÜÁ†ÅËá≥Â∞ëÂåÖÂê´‰∏Ä‰∏™Â≠óÊØçÂíå‰∏Ä‰∏™Êï∞Â≠óÔºåÂè™ËÉΩÂåÖÂê´Â≠óÊØçÂíåÊï∞Â≠óÔºå‰∏çËÉΩÂåÖÂê´ÁâπÊÆäÂ≠óÁ¨¶',\n          trigger: 'blur'\n        }],\n        confirmPassword: [{\n          required: true,\n          message: 'Á°ÆËÆ§ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          validator: (rule, value, callback) => {\n            if (value !== this.formData.password) {\n              callback(new Error('‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥'));\n            } else {\n              callback();\n            }\n          },\n          trigger: 'blur'\n        }],\n        description: [{\n          max: 100,\n          message: 'ÊèèËø∞‰∏çËÉΩË∂ÖËøá100‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    async register(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          document.getElementById('overlay').style.display = 'block';\n          const apiBaseUrl = process.env.VUE_APP_BACKEND_BASE_URL;\n          axios.post(`${apiBaseUrl}/register`, {\n            account: this.formData.account,\n            password: this.formData.password,\n            repeat_password: this.formData.confirmPassword,\n            // ‰º†ÈÄíÁ°ÆËÆ§ÂØÜÁ†Å‰ª•ËøõË°åÂåπÈÖçÈ™åËØÅ\n            name: this.formData.name,\n            phone: this.formData.phone,\n            email: this.formData.email,\n            description: this.formData.description\n          }).then(res => {\n            if (res.data.code === 200) {\n              this.$message({\n                showClose: true,\n                message: 'Ê≥®ÂÜåÊàêÂäü! Â∞ÜÂú®2ÁßíÂêéË∑≥ËΩ¨Ëá≥ÁôªÈôÜÈ°µ',\n                type: 'success'\n              });\n              let self = this;\n              setTimeout(() => {\n                this.$emit('trigger-login');\n                self.$router.push('/intro');\n              }, 5000);\n            } else {\n              this.$message({\n                showClose: true,\n                message: res.data.status,\n                type: 'error'\n              });\n              document.getElementById('overlay').style.display = 'none'; // Á°Æ‰øùoverlayË¢´ÈöêËóè\n            }\n          }).catch(error => {\n            console.error('Error creating user:', error);\n            this.$message({\n              showClose: true,\n              message: 'Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï',\n              type: 'error'\n            });\n          });\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    backLogin() {\n      this.$emit('trigger-login'); // Ëß¶ÂèëÁôªÂΩï‰∫ã‰ª∂\n    },\n    backGuest() {\n      this.$router.push('/intro');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","formData","account","name","phone","email","description","password","confirmPassword","rules","required","message","trigger","min","pattern","type","validator","rule","value","callback","Error","max","methods","register","formName","$refs","validate","valid","document","getElementById","style","display","apiBaseUrl","process","env","VUE_APP_BACKEND_BASE_URL","post","repeat_password","then","res","code","$message","showClose","self","setTimeout","$emit","$router","push","status","catch","error","console","log","backLogin","backGuest"],"sources":["src/components/registerPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"register-container\">\r\n        <div class=\"register-box\">\r\n            <div class=\"register-text\">\r\n                <h2>üëãÊ¨¢ËøéÊù•Âà∞HelloMeta!</h2>\r\n                <h3>Ê≥®ÂÜåË¥¶Âè∑Ôºå‰∏∫ÊÇ®ÁöÑ‰ΩúÂìÅËøõË°åÊï∞Â≠óÁ°ÆÊùÉ:</h3>\r\n                ‚ùïÔºöÊ†áÊúâ * ÁöÑ‰∏∫ÂøÖÂ°´È°π\r\n            </div>\r\n            <el-form :model=\"formData\" :rules=\"rules\" ref=\"formData\">\r\n                <div class=\"register-columns\">\r\n                    <!-- Á¨¨‰∏ÄÂàó -->\r\n                    <div class=\"column\">\r\n                        <div class=\"register-input\">\r\n                            <label>*ËÆæÁΩÆÁî®Êà∑id</label>\r\n                            <el-form-item prop=\"account\">\r\n                                <el-input type=\"text\" v-model=\"formData.account\"></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                        <div class=\"register-input\">\r\n                            <label>*ËÆæÁΩÆÁî®Êà∑Âêç</label>\r\n                            <el-form-item prop=\"name\">\r\n                                <el-input type=\"text\" v-model=\"formData.name\"></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                        <div class=\"register-input\">\r\n                            <label>*ËÆæÁΩÆÂØÜÁ†Å</label>\r\n                            <el-form-item prop=\"password\">\r\n                                <el-input type=\"password\" v-model=\"formData.password\" show-password></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                        <div class=\"register-input\">\r\n                            <label>*Á°ÆËÆ§ÂØÜÁ†Å</label>\r\n                            <el-form-item prop=\"confirmPassword\">\r\n                                <el-input type=\"password\" v-model=\"formData.confirmPassword\" show-password></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <!-- Á¨¨‰∫åÂàó -->\r\n                    <div class=\"column\">\r\n                        <div class=\"register-input\">\r\n                            <label>ËÆæÁΩÆÊâãÊú∫Âè∑</label>\r\n                            <el-form-item prop=\"phone\">\r\n                                <el-input type=\"text\" v-model=\"formData.phone\"></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                        <div class=\"register-input\">\r\n                            <label>ËÆæÁΩÆÈÇÆÁÆ±</label>\r\n                            <el-form-item prop=\"email\">\r\n                                <el-input type=\"text\" v-model=\"formData.email\"></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                        <div class=\"register-input\">\r\n                            <label>ËÆæÁΩÆÊèèËø∞</label>\r\n                            <el-form-item prop=\"description\">\r\n                                <el-input type=\"textarea\" v-model=\"formData.description\" rows=\"4\" autosize></el-input>\r\n                            </el-form-item>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <el-form-item>\r\n                    <button class=\"register-submit\" @click=\"register('formData')\">Ê≥®ÂÜå</button>\r\n                </el-form-item>\r\n            </el-form>\r\n            <!-- <a class=\"el-icon-back back-login\" @click=\"backLogin\"> Â∑≤ÊúâË¥¶Âè∑ÔºåÁÇπÊ≠§ÁôªÂΩï</a> -->\r\n            <a class=\"el-icon-back back-guest\" @click=\"backGuest\"> ËøîÂõûÊ∏∏ÂÆ¢ÊµèËßà</a>\r\n        </div>\r\n        <div class=\"overlay\" id=\"overlay\" style=\"display: none;\"></div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            formData: {\r\n                account: '',            // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ account Â≠óÊÆµ\r\n                name: '',               // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ name Â≠óÊÆµ\r\n                phone: '',              // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ phone Â≠óÊÆµ\r\n                email: '',              // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ email Â≠óÊÆµ\r\n                description: '',        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ description Â≠óÊÆµ\r\n                password: '',           // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ password Â≠óÊÆµ\r\n                confirmPassword: '',    // Áî®‰∫éÁ°ÆËÆ§ÂØÜÁ†ÅÂåπÈÖç\r\n            },\r\n            rules: {\r\n                account: [\r\n                    { required: true, message: 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    { min: 6, message: 'Áî®Êà∑ÂêçËá≥Â∞ëÈúÄË¶Å6‰∏™Â≠óÁ¨¶', trigger: 'blur' },\r\n                    { pattern: /^[A-Za-z0-9_]{6,}$/, message: 'Áî®Êà∑ÂêçÂè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠óÂíå‰∏ãÂàíÁ∫ø', trigger: 'blur' }\r\n                ],\r\n                name: [\r\n                    { required: true, message: 'ÂÖ¨Âè∏ÂêçÁß∞‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    { min: 3, message: 'ÂÖ¨Âè∏ÂêçÁß∞Ëá≥Â∞ëÈúÄË¶Å3‰∏™Â≠óÁ¨¶', trigger: 'blur' },\r\n                    { pattern: /^[A-Za-z0-9_\\u4e00-\\u9fa5]{3,}$/, message: 'ÂÖ¨Âè∏ÂêçÁß∞Âè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠ó„ÄÅ‰∏ãÂàíÁ∫øÂíå‰∏≠ÊñáÂ≠óÁ¨¶', trigger: 'blur' }\r\n                ],\r\n                phone: [\r\n                    // { required: true, message: 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    { pattern: /^1[3-9]\\d{9}$/, message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑', trigger: 'blur' }\r\n                ],\r\n                email: [\r\n                    // { required: true, message: 'ÈÇÆÁÆ±‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    { type: 'email', message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÈÇÆÁÆ±Âú∞ÂùÄ', trigger: 'blur' }\r\n                ],\r\n                password: [\r\n                    { required: true, message: 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    { min: 8, message: 'ÂØÜÁ†ÅËá≥Â∞ëÈúÄË¶Å8‰∏™Â≠óÁ¨¶', trigger: 'blur' },\r\n                    { pattern: /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/, message: 'ÂØÜÁ†ÅËá≥Â∞ëÂåÖÂê´‰∏Ä‰∏™Â≠óÊØçÂíå‰∏Ä‰∏™Êï∞Â≠óÔºåÂè™ËÉΩÂåÖÂê´Â≠óÊØçÂíåÊï∞Â≠óÔºå‰∏çËÉΩÂåÖÂê´ÁâπÊÆäÂ≠óÁ¨¶', trigger: 'blur' }\r\n                ],\r\n                confirmPassword: [\r\n                    { required: true, message: 'Á°ÆËÆ§ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\r\n                    {\r\n                        validator: (rule, value, callback) => {\r\n                            if (value !== this.formData.password) {\r\n                                callback(new Error('‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥'));\r\n                            } else {\r\n                                callback();\r\n                            }\r\n                        }, \r\n                        trigger: 'blur'\r\n                    }\r\n                ],\r\n                description: [\r\n                    { max: 100, message: 'ÊèèËø∞‰∏çËÉΩË∂ÖËøá100‰∏™Â≠óÁ¨¶', trigger: 'blur' }\r\n                ]\r\n            }\r\n        };\r\n    },\r\n    methods: {\r\n        async register(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    document.getElementById('overlay').style.display = 'block';\r\n                    const apiBaseUrl = process.env.VUE_APP_BACKEND_BASE_URL;\r\n                    axios.post(`${apiBaseUrl}/register`, {\r\n                        account: this.formData.account,\r\n                        password: this.formData.password,\r\n                        repeat_password: this.formData.confirmPassword, // ‰º†ÈÄíÁ°ÆËÆ§ÂØÜÁ†Å‰ª•ËøõË°åÂåπÈÖçÈ™åËØÅ\r\n                        name: this.formData.name,\r\n                        phone: this.formData.phone,\r\n                        email: this.formData.email,\r\n                        description: this.formData.description,\r\n                    })\r\n                    .then(res => {\r\n                        if (res.data.code === 200) {\r\n                            this.$message({\r\n                                showClose: true,\r\n                                message: 'Ê≥®ÂÜåÊàêÂäü! Â∞ÜÂú®2ÁßíÂêéË∑≥ËΩ¨Ëá≥ÁôªÈôÜÈ°µ',\r\n                                type