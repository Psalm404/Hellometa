{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      formData: {\n        account: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ account Â≠óÊÆµ\n        name: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ name Â≠óÊÆµ\n        phone: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ phone Â≠óÊÆµ\n        email: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ email Â≠óÊÆµ\n        description: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ description Â≠óÊÆµ\n        password: '',\n        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ password Â≠óÊÆµ\n        confirmPassword: '' // Áî®‰∫éÁ°ÆËÆ§ÂØÜÁ†ÅÂåπÈÖç\n      },\n      rules: {\n        account: [{\n          required: true,\n          message: 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          message: 'Áî®Êà∑ÂêçËá≥Â∞ëÈúÄË¶Å6‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^[A-Za-z0-9_]{6,}$/,\n          message: 'Áî®Êà∑ÂêçÂè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠óÂíå‰∏ãÂàíÁ∫ø',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞Ëá≥Â∞ëÈúÄË¶Å3‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^[A-Za-z0-9_\\u4e00-\\u9fa5]{3,}$/,\n          message: 'ÂÖ¨Âè∏ÂêçÁß∞Âè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠ó„ÄÅ‰∏ãÂàíÁ∫øÂíå‰∏≠ÊñáÂ≠óÁ¨¶',\n          trigger: 'blur'\n        }],\n        phone: [\n        // { required: true, message: 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n        {\n          pattern: /^1[3-9]\\d{9}$/,\n          message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑',\n          trigger: 'blur'\n        }],\n        email: [\n        // { required: true, message: 'ÈÇÆÁÆ±‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n        {\n          type: 'email',\n          message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÈÇÆÁÆ±Âú∞ÂùÄ',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          min: 8,\n          message: 'ÂØÜÁ†ÅËá≥Â∞ëÈúÄË¶Å8‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }, {\n          pattern: /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/,\n          message: 'ÂØÜÁ†ÅËá≥Â∞ëÂåÖÂê´‰∏Ä‰∏™Â≠óÊØçÂíå‰∏Ä‰∏™Êï∞Â≠óÔºåÂè™ËÉΩÂåÖÂê´Â≠óÊØçÂíåÊï∞Â≠óÔºå‰∏çËÉΩÂåÖÂê´ÁâπÊÆäÂ≠óÁ¨¶',\n          trigger: 'blur'\n        }],\n        confirmPassword: [{\n          required: true,\n          message: 'Á°ÆËÆ§ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫',\n          trigger: 'blur'\n        }, {\n          validator: (rule, value, callback) => {\n            if (value !== this.formData.password) {\n              callback(new Error('‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥'));\n            } else {\n              callback();\n            }\n          },\n          trigger: 'blur'\n        }],\n        description: [{\n          max: 100,\n          message: 'ÊèèËø∞‰∏çËÉΩË∂ÖËøá100‰∏™Â≠óÁ¨¶',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    async register(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          document.getElementById('overlay').style.display = 'block';\n          const apiBaseUrl = process.env.VUE_APP_BACKEND_BASE_URL;\n          axios.post(`${apiBaseUrl}/register`, {\n            account: this.formData.account,\n            password: this.formData.password,\n            repeat_password: this.formData.confirmPassword,\n            // ‰º†ÈÄíÁ°ÆËÆ§ÂØÜÁ†Å‰ª•ËøõË°åÂåπÈÖçÈ™åËØÅ\n            name: this.formData.name,\n            phone: this.formData.phone,\n            email: this.formData.email,\n            description: this.formData.description\n          }).then(res => {\n            if (res.data.code === 200) {\n              this.$message({\n                showClose: true,\n                message: 'Ê≥®ÂÜåÊàêÂäü! Â∞ÜÂú®2ÁßíÂêéË∑≥ËΩ¨Ëá≥ÁôªÈôÜÈ°µ',\n                type: 'success'\n              });\n              let self = this;\n              setTimeout(() => {\n                this.$emit('trigger-login');\n                self.$router.push('/intro');\n              }, 5000);\n            } else {\n              this.$message({\n                showClose: true,\n                message: res.data.status,\n                type: 'error'\n              });\n              document.getElementById('overlay').style.display = 'none'; // Á°Æ‰øùoverlayË¢´ÈöêËóè\n            }\n          }).catch(error => {\n            console.error('Error creating user:', error);\n            this.$message({\n              showClose: true,\n              message: 'Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï',\n              type: 'error'\n            });\n          });\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    backLogin() {\n      this.$emit('trigger-login'); // Ëß¶ÂèëÁôªÂΩï‰∫ã‰ª∂\n    },\n    backGuest() {\n      this.$router.push('/intro');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","formData","account","name","phone","email","description","password","confirmPassword","rules","required","message","trigger","min","pattern","type","validator","rule","value","callback","Error","max","methods","register","formName","$refs","validate","valid","document","getElementById","style","display","apiBaseUrl","process","env","VUE_APP_BACKEND_BASE_URL","post","repeat_password","then","res","code","$message","showClose","self","setTimeout","$emit","$router","push","status","catch","error","console","log","backLogin","backGuest"],"sources":["src/components/registerPage.vue"],"sourcesContent":["<template>\n    <div class=\"register-container\">\n        <div class=\"register-box\">\n            <div class=\"register-text\">\n                <h2>üëãÊ¨¢ËøéÊù•Âà∞HelloMeta!</h2>\n                <h3>Ê≥®ÂÜåË¥¶Âè∑Ôºå‰∏∫ÊÇ®ÁöÑ‰ΩúÂìÅËøõË°åÊï∞Â≠óÁ°ÆÊùÉ:</h3>\n                ‚ùïÔºöÊ†áÊúâ * ÁöÑ‰∏∫ÂøÖÂ°´È°π\n            </div>\n            <el-form :model=\"formData\" :rules=\"rules\" ref=\"formData\">\n                <div class=\"register-columns\">\n                    <!-- Á¨¨‰∏ÄÂàó -->\n                    <div class=\"column\">\n                        <div class=\"register-input\">\n                            <label>*ËÆæÁΩÆÁî®Êà∑id</label>\n                            <el-form-item prop=\"account\">\n                                <el-input type=\"text\" v-model=\"formData.account\"></el-input>\n                            </el-form-item>\n                        </div>\n                        <div class=\"register-input\">\n                            <label>*ËÆæÁΩÆÁî®Êà∑Âêç</label>\n                            <el-form-item prop=\"name\">\n                                <el-input type=\"text\" v-model=\"formData.name\"></el-input>\n                            </el-form-item>\n                        </div>\n                        <div class=\"register-input\">\n                            <label>*ËÆæÁΩÆÂØÜÁ†Å</label>\n                            <el-form-item prop=\"password\">\n                                <el-input type=\"password\" v-model=\"formData.password\" show-password></el-input>\n                            </el-form-item>\n                        </div>\n                        <div class=\"register-input\">\n                            <label>*Á°ÆËÆ§ÂØÜÁ†Å</label>\n                            <el-form-item prop=\"confirmPassword\">\n                                <el-input type=\"password\" v-model=\"formData.confirmPassword\" show-password></el-input>\n                            </el-form-item>\n                        </div>\n                    </div>\n\n                    <!-- Á¨¨‰∫åÂàó -->\n                    <div class=\"column\">\n                        <div class=\"register-input\">\n                            <label>ËÆæÁΩÆÊâãÊú∫Âè∑</label>\n                            <el-form-item prop=\"phone\">\n                                <el-input type=\"text\" v-model=\"formData.phone\"></el-input>\n                            </el-form-item>\n                        </div>\n                        <div class=\"register-input\">\n                            <label>ËÆæÁΩÆÈÇÆÁÆ±</label>\n                            <el-form-item prop=\"email\">\n                                <el-input type=\"text\" v-model=\"formData.email\"></el-input>\n                            </el-form-item>\n                        </div>\n                        <div class=\"register-input\">\n                            <label>ËÆæÁΩÆÊèèËø∞</label>\n                            <el-form-item prop=\"description\">\n                                <el-input type=\"textarea\" v-model=\"formData.description\" rows=\"4\" autosize></el-input>\n                            </el-form-item>\n                        </div>\n                    </div>\n                </div>\n                <el-form-item>\n                    <button class=\"register-submit\" @click=\"register('formData')\">Ê≥®ÂÜå</button>\n                </el-form-item>\n            </el-form>\n            <!-- <a class=\"el-icon-back back-login\" @click=\"backLogin\"> Â∑≤ÊúâË¥¶Âè∑ÔºåÁÇπÊ≠§ÁôªÂΩï</a> -->\n            <a class=\"el-icon-back back-guest\" @click=\"backGuest\"> ËøîÂõûÊ∏∏ÂÆ¢ÊµèËßà</a>\n        </div>\n        <div class=\"overlay\" id=\"overlay\" style=\"display: none;\"></div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    data() {\n        return {\n            formData: {\n                account: '',            // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ account Â≠óÊÆµ\n                name: '',               // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ name Â≠óÊÆµ\n                phone: '',              // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ phone Â≠óÊÆµ\n                email: '',              // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ email Â≠óÊÆµ\n                description: '',        // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ description Â≠óÊÆµ\n                password: '',           // ÁªëÂÆöÂà∞ÂêéÁ´ØÁöÑ password Â≠óÊÆµ\n                confirmPassword: '',    // Áî®‰∫éÁ°ÆËÆ§ÂØÜÁ†ÅÂåπÈÖç\n            },\n            rules: {\n                account: [\n                    { required: true, message: 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    { min: 6, message: 'Áî®Êà∑ÂêçËá≥Â∞ëÈúÄË¶Å6‰∏™Â≠óÁ¨¶', trigger: 'blur' },\n                    { pattern: /^[A-Za-z0-9_]{6,}$/, message: 'Áî®Êà∑ÂêçÂè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠óÂíå‰∏ãÂàíÁ∫ø', trigger: 'blur' }\n                ],\n                name: [\n                    { required: true, message: 'ÂÖ¨Âè∏ÂêçÁß∞‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    { min: 3, message: 'ÂÖ¨Âè∏ÂêçÁß∞Ëá≥Â∞ëÈúÄË¶Å3‰∏™Â≠óÁ¨¶', trigger: 'blur' },\n                    { pattern: /^[A-Za-z0-9_\\u4e00-\\u9fa5]{3,}$/, message: 'ÂÖ¨Âè∏ÂêçÁß∞Âè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠ó„ÄÅ‰∏ãÂàíÁ∫øÂíå‰∏≠ÊñáÂ≠óÁ¨¶', trigger: 'blur' }\n                ],\n                phone: [\n                    // { required: true, message: 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    { pattern: /^1[3-9]\\d{9}$/, message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑', trigger: 'blur' }\n                ],\n                email: [\n                    // { required: true, message: 'ÈÇÆÁÆ±‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    { type: 'email', message: 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÈÇÆÁÆ±Âú∞ÂùÄ', trigger: 'blur' }\n                ],\n                password: [\n                    { required: true, message: 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    { min: 8, message: 'ÂØÜÁ†ÅËá≥Â∞ëÈúÄË¶Å8‰∏™Â≠óÁ¨¶', trigger: 'blur' },\n                    { pattern: /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/, message: 'ÂØÜÁ†ÅËá≥Â∞ëÂåÖÂê´‰∏Ä‰∏™Â≠óÊØçÂíå‰∏Ä‰∏™Êï∞Â≠óÔºåÂè™ËÉΩÂåÖÂê´Â≠óÊØçÂíåÊï∞Â≠óÔºå‰∏çËÉΩÂåÖÂê´ÁâπÊÆäÂ≠óÁ¨¶', trigger: 'blur' }\n                ],\n                confirmPassword: [\n                    { required: true, message: 'Á°ÆËÆ§ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫', trigger: 'blur' },\n                    {\n                        validator: (rule, value, callback) => {\n                            if (value !== this.formData.password) {\n                                callback(new Error('‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥'));\n                            } else {\n                                callback();\n                            }\n                        }, \n                        trigger: 'blur'\n                    }\n                ],\n                description: [\n                    { max: 100, message: 'ÊèèËø∞‰∏çËÉΩË∂ÖËøá100‰∏™Â≠óÁ¨¶', trigger: 'blur' }\n                ]\n            }\n        };\n    },\n    methods: {\n        async register(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    document.getElementById('overlay').style.display = 'block';\n                    const apiBaseUrl = process.env.VUE_APP_BACKEND_BASE_URL;\n                    axios.post(`${apiBaseUrl}/register`, {\n                        account: this.formData.account,\n                        password: this.formData.password,\n                        repeat_password: this.formData.confirmPassword, // ‰º†ÈÄíÁ°ÆËÆ§ÂØÜÁ†Å‰ª•ËøõË°åÂåπÈÖçÈ™åËØÅ\n                        name: this.formData.name,\n                        phone: this.formData.phone,\n                        email: this.formData.email,\n                        description: this.formData.description,\n                    })\n                    .then(res => {\n                        if (res.data.code === 200) {\n                            this.$message({\n                                showClose: true,\n                                message: 'Ê≥®ÂÜåÊàêÂäü! Â∞ÜÂú®2ÁßíÂêéË∑≥ËΩ¨Ëá≥ÁôªÈôÜÈ°µ',\n                                type: 'success'\n                            });\n                            \n                            let self = this;\n                            setTimeout(() => {\n                                this.$emit('trigger-login'); \n                                self.$router.push('/intro');\n   